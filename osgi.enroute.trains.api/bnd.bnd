#
# OSGI ENROUTE TRAINS API
#

Bundle-Version:1.0.0.${tstamp}
Bundle-Description: 				\
	Contains the API code for the trains.

	
Export-Package: \
	osgi.enroute.trains.cloud.api;provide:=true,\
	osgi.enroute.trains.sensor.api,\
	osgi.enroute.trains.controller.api,\
	osgi.enroute.trains.train.api,\
	osgi.enroute.trains.hardware.api,\
	osgi.enroute.trains.operator.api,\
	osgi.enroute.trains.passenger.api,\
	osgi.enroute.trains.stations.api


Require-Capability: \
	compile-only

-buildpath: \
	osgi.enroute.base.api;version=1.0
